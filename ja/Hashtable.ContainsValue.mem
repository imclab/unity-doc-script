ハッシュテーブルに特定の値が格納されているか判定
CONVERTEXAMPLE
BEGIN EX
// ハッシュテーブルで値を検索

var h : Hashtable;
h = new Hashtable();
h.Add("1","one");
h.Add("2","two");
if(h.ContainsValue("one")) {
	Debug.Log("'one'の値は見つかりました!");
}
END EX
